{
  "schema_version": 1,
  "mixin": [
    "ad_astra.mixins.json",
    "ad_astra-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "com.github.alexnijjar.ad_astra",
    "id": "ad_astra",
    "version": "${version}",
    "name": "Ad Astra!",
    "description": "Live long and prosper, Ad Astra!",
    "authors": [
      "Alex Nijjar",
      "Barracuda",
      "Bunny",
      "Checheno",
      "CodexAdrian",
      "Facu",
      "Fizz",
      "OliviaTheVampire",
      "SpaceStar"
    ],
    "contact": {
      "homepage": "https://www.curseforge.com/minecraft/mc-mods/ad-astra",
      "sources": "https://github.com/AlexNijjar/Ad-Astra-Fabric"
    },
    "license": "MIT",
    "icon": "assets/ad_astra/icon.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "com.github.ad_astra.quilt.AdAstraQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.19"
      },
      {
        "id": "architectury",
        "version": ">=5.12.42"
      }
    ]
  }
}